import java.util.*;
public class Main {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		String a=sc.nextLine();
		int len=a.length();
		for(int i=0;i<len;i++) {
			for(int j=0;j<len-i;j++) {
				System.out.print(a.charAt(j));
			}
			System.out.println();
		}
		

	}

}
